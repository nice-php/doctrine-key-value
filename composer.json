{
  "name": "nice-php/doctrine-key-value",
  "description": "Integrates Doctrine ORM with Nice PHP framework",
  "license": "MIT",
  "require": {
    "php": ">=5.4.0",
    "doctrine/key-value-store": "dev-master",
    "symfony/config": "~2.3",
    "symfony/dependency-injection": "~2.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~3.7"
  },
  "autoload": {
    "psr-4": {
      "Nice\\Tests\\": "tests/",
      "Nice\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "beta",
  "prefer-stable": true
}
